<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appenders >
		<Console name="consoleLog" target="SYSTEM_OUT" folow="true">
			<PatternLayout pattern="%-20(%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread]) %-5level %logger{80} - %msg%n"></PatternLayout>
		</Console>
	</appenders>
	<loggers>
		<root level="debug">
			<appender-ref ref="consoleLog"/>
		</root>
	</loggers>
</configuration>